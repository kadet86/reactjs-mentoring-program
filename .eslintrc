{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "parser": "babel-eslint",
    "plugins": ["react"],
    "rules": {
        "max-len": [1, 120, 2, { "ignoreComments": true }],
        "react/prop-types": ["warn"]
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base"]
}
